{
	"name": "Remote Log Receiver",
	"description": "Remote log target receiver",
	"version": "0.1",
	"app": {
		"background": {
			"scripts": ["js/app/background.js"]
		}
	},

	"permissions": [
		{"socket": [
			"tcp-connect",
			"tcp-listen"
		]},

		{"fileSystem": [
			"write", "retainEntries", "directory"
		]}
	],

	"icons": { "16": "icons/receiver-16.png", "128": "icons/receiver-128.png" }
}